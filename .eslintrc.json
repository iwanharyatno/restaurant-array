{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jasmine": true,
        "codeceptjs/codeceptjs": true
    },
    "extends": [
      "airbnb-base",
      "plugin:codeceptjs/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "import/no-extraneous-dependencies": 0,
      "no-underscore-dangle": 0,
      "no-console": 0,
      "consistent-return": 0,
      "no-prototype-builtins": 0,
      "no-plusplus": 0
    },
    "plugins": [
      "jasmine",
      "codeceptjs"
    ]
}
